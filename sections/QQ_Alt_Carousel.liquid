{% endcapture %}
<div class="qq-section-wrap-{{sectionWidth}} {{sectionClass}} "
id="{{divID}}">
    <div class="qq-alternative-carousel-inner">
        <div>
            <div class="qq-alternative-carousel-header">
                {% if sectionTitle != blank %}
                    <div class="qq-title">{{sectionTitle}}</div>
                {% endif %}
                {% if sectionTextblock != blank %}
                    <div class="qq-textblock">{{sectionTextblock}}</div>
                {% endif %}
                {% if sectionButtonLabel != blank %}
                    {% capture sectionButton %}
                    <div class="qq-section-button">
                        <div class="qq-button-wrapper">
                            <a class="qq-btn btn btn_zoom" href="{{sectionButtonUrl}}"{{sectionButtonAttributes}}><span>{{sectionButtonLabel}}</span></a>
                        </div>
                    </div>
                    {% endcapture %}
                    {{sectionButton}}
                {% endif %}
            </div>
        </div>
        <div class="qq-alternative-carousel-blocks"
        data-showTitles="{{showTitles}}"
        data-slidesToShow="{{slidesToShow}}"
        data-sectionWidth="{{sectionWidth}}"
        data-carouselAutoplay="{{carouselAutoplay}}"
        data-carouselSpeed="{{carouselSpeed}}"
        data-carouselMobileBlocks="{{slidesToShowMobile}}"
        data-carouselResize="{{resizeSlider}}">
            {% for block in section.blocks %}
                {% assign blockAttributes = block.shopify_attributes %}          
                {% if block.type == 'collection' %}
                    {% assign collection = collections[block.settings.collection] %}
                    {% assign collectionLimit = block.settings.collection_limit %}
                    {% for product in collection.products limit: collectionLimit %}
                        {% liquid 
                            assign blockTitle = product.title
                            assign blockImage = product.images.first
                            assign blockButtonUrl = product.url  
                            if blockImage != blank 
                                if blockImage.alt != blank 
                                    assign blockImageAlt = 'alt="' | append: blockImage.alt | append: '"'
                                else 
                                    assign blockImageAlt = ''
                                endif
                                assign proportion = blockImage.aspect_ratio
                                assign imageImgW = blockImage.width
                                assign imageImgH = blockImage.height
                                assign maxWidthAllowed = 500
                                assign maxHeightAllowed = 1300
                                if slidesToShow == 2
                                        assign maxWidthAllowed = 850
                                        assign maxHeightAllowed = 1300
                                elsif slidesToShow == 3
                                        assign maxWidthAllowed = 550
                                        assign maxHeightAllowed = 850
                                elsif slidesToShow == 4
                                        assign maxWidthAllowed = 450
                                        assign maxHeightAllowed = 650
                                elsif slidesToShow == 5
                                        assign maxWidthAllowed = 350
                                        assign maxHeightAllowed = 500
                                elsif slidesToShow == 6
                                        assign maxWidthAllowed = 280
                                        assign maxHeightAllowed = 450
                                endif
                                if proportion > 1
                                    # landscape
                                    assign mostW = maxWidthAllowed
                                    assign mostH = mostW | times: proportion
                                elsif proportion < 1
                                    # portrait
                                    assign mostH = maxHeightAllowed
                                    assign mostW = mostH | times: proportion
                                else
                                    # sqaure
                                    assign mostW = maxWidthAllowed
                                    assign mostH = mostW
                                endif
                                assign iw = imageImgW | at_most: mostW | round
                                assign ih = imageImgH | at_most: mostH | round
                                if imageSize == 'portrait'
                                    assign ih = iw | divided_by: 0.6666667 | round
                                    if imageImgH < ih
                                        assign ih = imageImgH
                                        assign iw = ih | divided_by: 1.5 | round
                                    endif
                                    if imageImgW < iw
                                        assign iw = imageImgW
                                        assign ih = iw | divided_by: 0.666667 | round
                                    endif
                                elsif imageSize == 'square'
                                    assign ih = iw
                                    if iw > imageImgW
                                        assign iw = imageImgW
                                        assign ih = iw
                                    endif
                                    if ih > imageImgH
                                        assign ih = imageImgH
                                        assign iw = ih
                                    endif
                                elsif imageSize == 'original'
                                    assign ih = iw | divided_by: proportion | round
                                endif
                                assign size = iw | append: 'x' | append: ih
                                assign blockImageUrl = blockImage | img_url: size, crop: 'center'                           
                            endif 
                        %}
                        <div class="qq-alternative-carousel-block" {{blockAttributes}}>
                            <div class="qq-block-image qq-{{imageSize}}" data-imageSize="{{imageSize}}">
                                {% if blockImage != blank %}
                                    <img src="{{blockImageUrl}}" {{blockImageAlt}} loading="lazy">
                                {% else %}
                                    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 771 771" style="width:100%;height:100%;" preserveAspectRatio="xMidYMid slice"><g style="isolation:isolate"><g id="Layer_2" data-name="Layer 2"><g id="Layer_1-2" data-name="Layer 1" style="fill:#efefef;"><rect width="771" height="771" style="fill:#efefef;"/><g style="opacity:0.5"><path d="M280.73,441.53V406.12h-.48c-3,6.78-8.91,10.7-17.23,10.7-14.49,0-23-11.77-23-30.42S248.41,356,263.14,356c8.79,0,14.74,4,17.59,11.28h.47l.6-9.86H290v84.13Zm0-53.71V385c0-12.83-6.42-20.44-16-20.44s-15.1,7.72-15.1,21.87,5.47,21.86,15.1,21.86S280.73,400.66,280.73,387.82Z" style="fill:#373833"/><path d="M302.48,357.4h8.91l7.72,44.09h.48l10-44.09h7.49l10,44.09h.47l7.73-44.09h8.55l-10.81,58h-9.87l-9.74-43h-.47l-9.75,43h-9.86Z" style="fill:#373833"/><path d="M376.86,407.19h20V365.6h-20v-8.2h29.23v49.79h20v8.2h-49.2Zm15.21-69.52a8.08,8.08,0,1,1,8.08,8.09A7.87,7.87,0,0,1,392.07,337.67Z" style="fill:#373833"/><path d="M471.45,441.53V406.12H471c-3,6.78-8.92,10.7-17.23,10.7-14.5,0-23.06-11.77-23.06-30.42S439.13,356,453.86,356c8.8,0,14.74,4,17.59,11.28h.48l.59-9.86h8.2v84.13Zm0-53.71V385c0-12.83-6.41-20.44-16-20.44s-15.09,7.72-15.09,21.87,5.47,21.86,15.09,21.86S471.45,400.66,471.45,387.82Z" style="fill:#373833"/><path d="M492.72,386.4c0-19.73,8.8-30.42,25.19-30.42s25.2,10.69,25.2,30.42-8.8,30.42-25.2,30.42S492.72,406.12,492.72,386.4Zm40.76,0c0-14.38-5.35-21.87-15.57-21.87S502.35,372,502.35,386.4s5.35,21.86,15.56,21.86S533.48,400.77,533.48,386.4Z" style="fill:#373833"/><path d="M553.09,386.4c0-18.66,8.55-30.42,23-30.42,8.32,0,14.26,3.92,17.23,10.69h.48V331h9.27v84.37h-8.2l-.6-9.86h-.47c-2.85,7.25-8.8,11.29-17.59,11.29C561.52,416.82,553.09,405.17,553.09,386.4Zm40.76,1.42V385c0-12.83-6.42-20.44-16.05-20.44s-15.09,7.72-15.09,21.87,5.47,21.86,15.09,21.86S593.85,400.66,593.85,387.82Z" style="fill:#373833"/><path d="M615.36,386.28c0-19.61,8.79-30.3,25.31-30.3S666,366.67,666,386.28a18.15,18.15,0,0,1-.12,2.49H625c.59,13,6.06,19.49,16.52,19.49,6.53,0,11-2.61,13.43-7.6h10.34C661.94,411,654,416.82,641.38,416.82,624.39,416.82,615.36,406.24,615.36,386.28Zm40.75-5.35c-1.3-10.81-6.53-16.4-15.44-16.4s-14.15,5.59-15.45,16.4Z" style="fill:#373833"/><polygon points="105.12 429.87 179.13 385.56 105.12 341.26 115.51 324 201.41 375.33 201.41 395.8 115.51 447.13 105.12 429.87" style="fill:#29abe2"/><polygon points="119.25 395.8 119.25 375.33 205.22 323.97 215.55 341.26 141.53 385.56 215.55 429.87 205.16 447.13 119.25 395.8" style="fill:#0071bc;mix-blend-mode:multiply"/></g></g></g></g></svg>
                                {% endif %}
                            </div>
                            {% if blockTitleDisplay == true %}
                                <div class="qq-block-title">
                                    <{{blockTitleType}}>{{blockTitle}}</{{blockTitleType}}>
                                </div>
                            {% endif %}        
                            {% if productPriceDisplay == true %}
                                {% assign productPrice = product.price %}
                                {% assign productComparePrice = product.compare_at_price_max %}
                                <span class="qq-product-price {% if product.compare_at_price_min > product.price_min %}has-compare-price{% endif %}">
                                    {% comment %} {{productPrice | money }} {% endcomment %}
                                    {% if product.price_varies and product.variants.size > 1 %}
                                        {% if productPriceFrom == true %}
