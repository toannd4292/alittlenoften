                assign numOfProducts = collectionLimit
            endif
            assign blocksAmount = blocksAmount | plus: numOfProducts | minus: 1
        endif 
    endfor
    assign resizeSlider = false
    if sectionWidth == 'fullwidth'
        if blocksAmount > slidesToShow
            comment
                assign slidesToShow = slidesToShow | plus: 1
            endcomment
            assign resizeSlider = true
        endif
    endif
    if blocksAmount > 3
        assign mobileBlocks = blocksAmount | divided_by: 2
    elsif blocksAmount <= 3
        assign mobileBlocks = 1
    else
        assign mobileBlocks = blocksAmount
    endif
